<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.MultiDev.hopeapp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/reporteSintomas"
        android:name="com.MultiDev.hopeapp.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/visorPacientes"
        android:name="com.MultiDev.hopeapp.GUIVisores.VisorPacientes"
        android:label="Visor de Pacientes Sin tutela"
        tools:layout="@layout/fragment_visor_pacientes" />
    <fragment
        android:id="@+id/miProgreso"
        android:name="com.MultiDev.hopeapp.MiProgreso"
        android:label="fragment_mi_progreso"
        tools:layout="@layout/fragment_mi_progreso" />
    <fragment
        android:id="@+id/historialSintomas"
        android:name="com.MultiDev.hopeapp.HistorialSintomas"
        android:label="fragment_historial_sintomas"
        tools:layout="@layout/fragment_historial_sintomas" />
    <fragment
        android:id="@+id/miCuenta"
        android:name="com.MultiDev.hopeapp.MiCuenta"
        android:label="Mi cuenta"
        tools:layout="@layout/fragment_mi_cuenta" />
    <fragment
        android:id="@+id/opcSalir"
        android:name="com.MultiDev.hopeapp.FragmenSalir"
        android:label="Saliendo"
        tools:layout="@layout/fragment_fragmen_salir">
        <action
            android:id="@+id/action_opcSalir_self"
            app:destination="@id/opcSalir"
            app:enterAnim="@anim/introduce_fade" />
    </fragment>
    <fragment
        android:id="@+id/visorSintomas"
        android:label="fragment_visor_sintomas_pacientes"
        tools:layout="@layout/fragment_visor_sintomas_pacientes" />
</navigation>